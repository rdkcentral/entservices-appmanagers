precondition = ["Platform"]
callsign = "org.rdk.AppStorageManager"
autostart = "@PLUGIN_APP_STORAGE_MANAGER_AUTOSTART@"
startuporder = "@PLUGIN_APP_STORAGE_MANAGER_STARTUPORDER@"

configuration = JSON()

rootobject = JSON()
rootobject.add("mode", "@PLUGIN_APP_STORAGE_MANAGER_MODE@")
rootobject.add("locator", "lib@PLUGIN_IMPLEMENTATION@.so")
configuration.add("root", rootobject)
configuration.add("path", "@PLUGIN_APP_STORAGE_MANAGER_PATH@")
